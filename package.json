{
  "name": "rockethub",
  "version": "0.1.0",
  "private": true,
  "author": "ethec",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "emulate": "yarn build && cd functions && yarn build && cd .. && firebase emulators:start --import ./seed-data",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
