    "@reduxjs/toolkit": "^1.8.2",
    "@tanstack/react-query": "^4.18.0",
    "@types/react-datepicker": "^4.3.4",
    "@types/uuid": "^8.3.4",
    "express": "^4.18.2",
    "firebase": "^9.6.7",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.1",
    "framer-motion": "^5.6.0",
    "next": "12",
    "next-pwa": "^5.4.4",
    "next-seo": "^5.1.0",
